// database
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider             = "python3 -m prisma"
  interface            = "asyncio"
  recursive_type_depth = 5
}

model SmsLog {
  id        String   @id @default(uuid())
  name      String
  photo     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt()
}
